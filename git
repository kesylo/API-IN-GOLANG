# add files to git
git add *

# Commit changes
git commit -m "message"

# push to repo
git push origin avni

# create a branch
git branch loic

#################### Branch info ####################################

# display all branches
git branch

# Switch to a branch
git checkout loic

# Show changes
git status

# push to branch
git push -u origin loic

# push with ssh
git remote set-url origin git@github.com:kesylo/API-IN-GOLANG.git
